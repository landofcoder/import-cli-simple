{
    "name": "landofcoder/import-cli-simple",
    "description": "CLI providing single-threaded Magento 2 importing functionality based on Pacemaker",
    "license": "MIT",
    "require": {
        "php": ">=7.3.0"
    },
    "version": "4.2.1",
    "require-dev": {
        "doctrine/dbal": "2.5.*",
        "pdepend/pdepend": "^2.5.2",
        "phpmd/phpmd": "^2.11.0",
        "phpunit/phpunit": "^6.5.0|^8.0.0|~9.5.0",
        "sebastian/phpcpd": "~3.0|~4.0|~5.0|~6.0",
        "squizlabs/php_codesniffer": "~3.4.0|~3.6.0",
        "consolidation/robo": "~1.0",
        "mikey179/vfsstream": "~1.0",
        "symfony/dotenv": "~3.0|~4.0",
        "symfony/http-kernel": "~2.0|~3.0|~4.0",
        "behat/mink-extension": "2.3.*",
        "dmore/chrome-mink-driver": "2.7.*",
        "dmore/behat-chrome-extension": "1.3.*",
        "behat/mink-goutte-driver": "1.1.*",
        "behat/symfony2-extension": "dev-master as 3.0.0",
        "phpcompatibility/php-compatibility": "*",
        "techdivision/import-sample-data": "^4.3.5"
    },
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "t.wagner@techdivision.com"
        },
        {
            "name": "Vadim Justus",
            "email": "v.justus@techdivision.com"
        },
        {
            "name": "Marcus Döllerer",
            "email": "m.doellerer@techdivision.com"
        },
        {
            "name": "David Führ",
            "email": "d.fuer@techdivision.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "TechDivision\\Import\\Cli\\Simple\\": [
                "tests/integration/",
                "tests/acceptance/bootstrap/"
            ]
        }
    },
    "bin": [
        "bin/import-simple",
        "bin/import-simple.php"
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    }
}
